# {project}.tproj (template manifest)
project.name: {project-name-placeholder}
project.id: P-001
author: hannahbellesheart
created_at: 2025-10-22T12:38:05Z
specs_path: .trac/specs/
logs_path: .trac/logs/

# Controls whether an agent is allowed to perform live network lookups for rate/estimate data
# true = agent may fetch rates from allowed_rate_sources when generating scope estimates
# false = agent must mark all costs PROVISIONAL and create a Decision Point to request permission
fetch_rates_on_generate: false

# If fetch_rates_on_generate is true, agent must use ONLY the allowed_rate_sources listed below.
allowed_rate_sources:
  - https://clutch.co/
  - https://www.goodfirms.co/
  - https://www.upwork.com/
  - https://www.toptal.com/
  - https://www.freelancer.com/
  - https://www.glassdoor.com/
  - https://www.indeed.com/
  - https://www.linkedin.com/salary/
  - https://www.roberthalf.com/
  - https://insights.stackoverflow.com/survey

# Whether generated artifacts may be committed to repo by the agent
# false = generated artifacts should be produced at CI/build time and not committed
# true = agent may commit generated artifacts when a Decision Point and PR review approve it
commit_generated_artifacts: false

preferred_sdk_languages: []  # agent should prompt user if empty on first run; user may fill e.g. ["typescript","python"]

rates:
  # fallback provisional rates agent may use if external lookups are disabled or fail
  low: 50
  standard: 120
  high: 180

notes: |
  - Replace project.name with canonical name when decided.
  - Any change to fetch_rates_on_generate or allowed_rate_sources must be recorded as Decision Point D-### and logged.
  - Agents must append a session log entry whenever they fetch external data.
